<local:CustomWindow x:Class="База_артикулов.Формы.WindowEdit"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:База_артикулов.Формы" xmlns:редактирование="clr-namespace:База_артикулов.Формы.Страницы.Редактирование"
                    mc:Ignorable="d"
        Title="WindowEdit" SizeToContent="WidthAndHeight" Loaded="Window_Loaded" Height="300" Width="300">
    <Grid ClipToBounds="True">
        <Frame x:Name="fMain" Content="Frame" HorizontalAlignment="Stretch"
               VerticalAlignment="Stretch"             
               NavigationUIVisibility="Hidden" ScrollViewer.VerticalScrollBarVisibility="Disabled" 
               HorizontalContentAlignment="Stretch" 
               VerticalContentAlignment="Stretch"/>
    </Grid>
</local:CustomWindow>
